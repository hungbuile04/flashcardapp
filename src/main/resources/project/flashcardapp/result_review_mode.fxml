<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@stylesheet/result_review_mode.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.flashcardapp.Controller.LearningMode.ResultReviewModeController">
   <children>
      <Label fx:id="resultLabel" layoutX="45.0" layoutY="15.0" onMouseClicked="#backButtonClicked" prefHeight="100.0" prefWidth="510.0" text="Congratulations!  &#10;You have finished this deck for now.&#10;&#10;&#10;">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="235.0" layoutY="330.0" mnemonicParsing="false" onMouseClicked="#backButtonClicked" prefHeight="41.0" prefWidth="131.0" text="BACK">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Pane fx:id="quotePane" layoutX="113.0" layoutY="166.0" prefHeight="112.0" prefWidth="375.0">
         <children>
            <Label fx:id="quoteLabel" layoutY="1.0" prefHeight="66.0" prefWidth="372.0" text="I can accept failure, everyone fails at something. But I can't accept not trying." wrapText="true" />
            <Label fx:id="authorLabel" layoutX="237.0" layoutY="66.0" prefHeight="41.0" prefWidth="131.0" text="-Micheal Jordan-" wrapText="true" />
         </children>
      </Pane>
   </children>
</AnchorPane>
